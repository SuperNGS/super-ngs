{
  /* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
  "rules": {
    ".read": false,
    ".write": false,
    "public": {
        ".read": true,
    		".write": false
    },
    "configuration": {
      ".read": "auth.admin === true",
      ".write": "auth.admin == true"
    }
  }
}